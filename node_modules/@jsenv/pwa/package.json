{
  "name": "@jsenv/pwa",
  "version": "5.0.0",
  "description": "Service worker and other progressive web application helpers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/pwa"
  },
  "engines": {
    "node": ">=16.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./main.js"
    },
    "./*": "./*"
  },
  "main": "./main.js",
  "files": [
    "/src/",
    "/main.js"
  ],
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "dev": "node ./script/dev/dev.mjs",
    "eslint": "node ./node_modules/eslint/bin/eslint.js . --ext=.js,.mjs,.jsx,.html --ignore-path=.eslintignore",
    "test": "node ./script/test/test.mjs",
    "test-with-coverage": "npm run test -- --coverage",
    "prettier": "prettier --write .",
    "packages-install": "cd docs/react_example && npm install",
    "certificate-install": "node ./script/dev/install_certificate.mjs"
  },
  "devDependencies": {
    "@jsenv/assert": "2.5.3",
    "@jsenv/core": "27.0.0-alpha.12",
    "@jsenv/eslint-config": "16.0.9",
    "@jsenv/file-size-impact": "12.1.10",
    "@jsenv/github-release-package": "1.3.5",
    "@jsenv/https-local": "1.0.8",
    "@jsenv/eslint-import-resolver": "0.1.4",
    "@jsenv/package-publish": "1.7.2",
    "eslint": "8.13.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.26.0",
    "playwright": "1.21.0",
    "prettier": "2.6.2",
    "react": "18.0.0"
  }
}
